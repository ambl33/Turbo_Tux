<div class="border-bottom">
  <div class = "d-flex justify-content-between align-items-center container p-3">
    <p>My Bookings</p>
    <h1>Turbo Tux</h1>
    <p>User Account</p>
  </div>
</div>


<div class="container-index">
<div class="card-grid">
  <% @tuxes.each do |tux| %>
    <%= link_to tux_path(tux) do %>
     <div class="card">
       <%= image_tag 'fabio_tux.jpg' %>
       <h1><%= tux.name %></h1>
       <p class="price">$<%= tux.price %>0</p>
      <p><%= tux.description.truncate(120)%></p>
      <p><button>Book Now</button></p>
     </div>   
    <%end %>
  <% end %>
</div>
</div>






